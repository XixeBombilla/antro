<h1>Editar foto</h1>

<% form_for(@foto, :html => { :multipart => true }) do |f| %>

  <%= f.error_messages %>

  <p>
    <%= f.label :titulo %><br />
    <%= f.text_field :titulo %>
  </p>
  <p>
    <%= f.label :imagen %><br />
    <%= f.file_field :imagen %>
  </p>
  <p>
    <%= f.submit 'Actualizar' %>
  </p>
<% end %>

<%= link_to 'Ver', @foto %> |
<%= link_to 'Regresar', fotos_path %>